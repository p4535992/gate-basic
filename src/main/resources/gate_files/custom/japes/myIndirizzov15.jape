		 (
		  (CONJ_UNIVERSAL_ITA)[1,3]	
		  ({Token.string==~"[Aa]ula"}|{Token.string==~"[Ss]tanza"})[1,1]
		  (CONJ_UNIVERSAL_ITA)[0,3]	
		  ({Token.kind==number})
		 )[0,1]	  
	  )[0,1]
	  /*84011 AMALFI SA*/
	  (
	   (CONJ_UNIVERSAL_ITA)[0,3]
	   ({Token.kind == number,Token.length==5})[1,1]
	  )[0,1]
	  /* 2° piano (dopo il prefisso postale)*/
