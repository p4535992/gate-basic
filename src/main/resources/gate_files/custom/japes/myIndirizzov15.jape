		(CONJ_UNIVERSAL_ITA)[1,3]
		 ({Token.kind==word,!Lookup.majorType == localita}|{Token.kind==number})
		 ({Token.string=="°"})[0,1]
		 (CONJ_UNIVERSAL_ITA)[0,3]
